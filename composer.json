{
    "name": "rapltech/laravel-sms",
    "type": "library",
    "description": "Package to support multi SMS gateway providers",
    "license": "MIT",
    "keywords": ["laravel", "sms"],
    "homepage": "https://github.com/rapltech/laravel-sms",
    "authors": [
        {
            "name": "Vignesh Gurusamy",
            "email": "vignesh@getrapl.com"
        },
        {
            "name": "Meyyappan Velayutham",
            "email": "meyyappan@getrapl.com"
        }
    ],
    "require": {
        "php": "^8.5",
        "illuminate/support": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Rapltech\\LaravelSms\\": "src"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^12.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Rapltech\\LaravelSms\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapltech\\LaravelSms\\Providers\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms": "Rapltech\\LaravelSms\\Facades\\Sms"
            }
        }
    }
}
